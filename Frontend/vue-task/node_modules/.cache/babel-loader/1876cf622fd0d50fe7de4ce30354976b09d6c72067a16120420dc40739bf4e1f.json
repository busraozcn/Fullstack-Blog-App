ns-serif;\n  --font-size: 18px;\n  --font-weight: 400;\n  --line-height: 27px;\n  --text-color: #00000073;\n}\n\n.homepage {\n  display: flex;\n}\n\n.sidebar {\n  color: var(--text-color);\n  background-color: var(--sidebar-bg-color);\n  position: fixed;\n  z-index: 1;\n  top: 0;\n  left: 0;\n  bottom: 0;\n  display: flex;\n  flex-direction: column;\n  width: 250px;\n  padding: 32px 0px 32px 0px;\n  gap: 0px;\n  border-right: 1px solid #d8d9dd;\n}\n\n.menu {\n  list-style: none;\n  padding: 0;\n  margin: 0;\n  flex-grow: 1;\n}\n\n.menu-item {\n  margin-left: 5px;\n  margin-bottom: 0.5em;\n}\n\n.menu-link {\n  font-family: var(--font-family);\n  font-size: var(--font-size);\n  font-weight: var(--font-weight);\n  line-height: var(--line-height);\n  color: var(--text-color);\n  text-decoration: none;\n  display: block;\n  padding: 0.5em 1em;\n  border-radius: 0.25em;\n  transition: background-color 0.3s;\n}\n\n.menu-link:hover {\n  background-color: var(--sidebar-item-hover);\n  color: black;\n}\n\n.footer {\n  display: flex;\n  align-items: center;\n  margin-top: auto;\n  font-family: var(--font-family);\n  font-size: var(--font-size);\n  font-weight: var(--font-weight);\n  line-height: var(--line-height);\n  color: var(--text-color);\n}\n\n.footer-logo {\n  width: 50px;\n  height: auto;\n  margin-right: 0.5em;\n}\n\n.user-container {\n  display: flex;\n  flex-direction: column;\n  align-items: flex-start;\n  padding-left: 250px;\n  margin-left: 20px;\n}\n\n.profile {\n  width: 358.67px;\n  background-color: #ffffff;\n  padding: 20px 24px;\n  gap: 10px;\n  border-radius: 12px;\n  border: 1px solid #d8d9dd;\n  margin-bottom: 20px;\n  cursor: pointer;\n}\n.profile-header {\n  display: flex;\n  align-items: center;\n  margin-bottom: 20px;\n}\n\n.profile-img {\n  width: 100px;\n  height: 100px;\n  border-radius: 50%;\n  margin-bottom: 10px;\n}\n\n.user-info {\n  text-align: left;\n  margin-left: 20px;\n}\n.profile-img {\n  width: 100px;\n  height: 100px;\n  border-radius: 50%;\n  margin-right: 20px;\n}\n\nh2 {\n  font-size: 18px;\n  font-family: Poppins;\n  font-weight: 500;\n  line-height: 1.2;\n}\n.location-info,\n.company-info,\n.website-info {\n  margin-bottom: 10px;\n}\n\nh3 {\n  font-family: Poppins;\n  font-size: 14px;\n  font-weight: 500;\n  line-height: 1.5;\n  margin-bottom: 5px;\n}\n\np {\n  font-family: Poppins;\n  font-size: 14px;\n  line-height: 1.5;\n  margin: 0;\n}\n.infos {\n  text-align: left;\n}\n.infos > div {\n  margin-bottom: 10px;\n}\n.box-shadow {\n  box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.1);\n}\n.user-container:hover .profile {\n  background-color: #f0f0f0;\n}\n</style>\n"],"mappings":";;EACOA,KAAK,EAAC;AAAU;;EACdA,KAAK,EAAC;AAAS;gCAClBC,mBAAA,CAIK;EAJDD,KAAK,EAAC;AAAM,I,aACdC,mBAAA,CAEK;EAFDD,KAAK,EAAC;AAAW,I,aACnBC,mBAAA,CAAuC;EAApCC,IAAI,EAAC,GAAG;EAACF,KAAK,EAAC;GAAY,OAAK,E;;EAGlCA,KAAK,EAAC;AAAQ;mBARzB;gCAcQC,mBAAA,CAAoB,cAAd,SAAO;;EAGZD,KAAK,EAAC;AAAgB;;EAElBA,KAAK,EAAC;AAAgB;mBAnBnC;;EAyBeA,KAAK,EAAC;AAAW;;EAMnBA,KAAK,EAAC;AAAO;;EACXA,KAAK,EAAC;AAAe;iCACxBC,mBAAA,CAAiB,YAAb,UAAQ;;EAGTD,KAAK,EAAC;AAAc;iCACvBC,mBAAA,CAAgB,YAAZ,SAAO;;EAGRD,KAAK,EAAC;AAAc;iCACvBC,mBAAA,CAAgB,YAAZ,SAAO;;uBAxCrBE,mBAAA,CA8CM,OA9CNC,UA8CM,GA7CJH,mBAAA,CAcM,OAdNI,UAcM,GAbJC,UAIK,EACLL,mBAAA,CAOM,OAPNM,UAOM,GANJN,mBAAA,CAIE;IAHCO,GAAG,EAAEC,OAAO;IACbC,GAAG,EAAC,aAAa;IACjBV,KAAK,EAAC;0BAZhBW,UAAA,GAcQC,UAAoB,C,KAGxBX,mBAAA,CA6BM,OA7BNY,UA6BM,I,kBA5BJV,mBAAA,CA2BMW,SAAA,QA7CZC,WAAA,CAkB0BC,KAAA,CAAAC,KAAK,EAAbC,IAAI;yBAAhBf,mBAAA,CA2BM;MA3BsBgB,GAAG,EAAED,IAAI,CAACE,EAAE;MAAEpB,KAAK,EAAC;QAC9CC,mBAAA,CAWM,OAXNoB,UAWM,GAVJpB,mBAAA,CAIE;MAHCO,GAAG,EAAEC,OAAO;MACbC,GAAG,EAAC,iBAAiB;MACrBV,KAAK,EAAC;4BAvBlBsB,UAAA,GAyBUrB,mBAAA,CAIM,OAJNsB,WAIM,GAHJtB,mBAAA,CAAwB,YAAAuB,gBAAA,CAAjBN,IAAI,CAACO,IAAI,kBAChBxB,mBAAA,CAAuB,WAAAuB,gBAAA,CAAjBN,IAAI,CAACQ,KAAK,kBAChBzB,mBAAA,CAAuB,WAAAuB,gBAAA,CAAjBN,IAAI,CAACS,KAAK,iB,KAGpB1B,mBAAA,CAaM,OAbN2B,WAaM,GAZJ3B,mBAAA,CAGM,OAHN4B,WAGM,GAFJC,WAAiB,EACjB7B,mBAAA,CAAyD,WAAAuB,gBAAA,CAAnDN,IAAI,CA