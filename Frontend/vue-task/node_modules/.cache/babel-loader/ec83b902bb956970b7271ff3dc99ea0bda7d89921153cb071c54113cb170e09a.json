    }\r\n    };\r\n\r\n    const openModal = (photo) => {\r\n      selectedPhoto.value = photo;\r\n      showModal.value = true;\r\n    };\r\n\r\n    const closeModal = () => {\r\n      showModal.value = false;\r\n      selectedPhoto.value = null;\r\n    };\r\n\r\n    onMounted(() => {\r\n      const albumId = route.params.albumId;\r\n      const userId = route.query.userId;\r\n      fetchAlbumAndPhotos(albumId);\r\n      fetchUser(userId);\r\n    });\r\n\r\n    return {\r\n      album,\r\n      photos,\r\n      showModal,\r\n      selectedPhoto,\r\n      goBack,\r\n      openModal,\r\n      closeModal,\r\n      selectedUser,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.album-detail-page {\r\n  display: flex;\r\n}\r\n\r\n.content {\r\n  margin-left: 260px;\r\n  padding: 20px;\r\n}\r\n\r\n.photos-list {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 20px;\r\n}\r\n\r\n.photo-card {\r\n  width: 200px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n  cursor: pointer;\r\n}\r\n\r\n.photo-card img {\r\n  width: 100%;\r\n  height: 150px;\r\n  object-fit: cover;\r\n}\r\n</style>\r\n"],"mappings":";OAMUA,UAAmD;;EALtDC,KAAK,EAAC;AAAmB;;EAEvBA,KAAK,EAAC;AAAS;gCAEhBC,mBAAA,CAIE;EAHAC,GAAmD,EAAnDH,UAAmD;EACnDI,GAAG,EAAC,cAAc;EAClBH,KAAK,EAAC;;gCAERC,mBAAA,CAA0C;EAApCD,KAAK,EAAC;AAAe,GAAC,SAAO;oBALnCI,UAIE,EACFC,UAA0C,C;;EAGvCL,KAAK,EAAC;AAAa;mBAb9B;mBAAA;;;;uBACEM,mBAAA,CA6BM,OA7BNC,UA6BM,GA5BJC,YAAA,CAAyCC,2BAAA;IAAtBC,IAAI,EAAEC,MAAA,CAAAC;EAAY,mCACrCX,mBAAA,CAoBM,OApBNY,UAoBM,GAnBJZ,mBAAA,CAOM;IAPDD,KAAK,EAAC,UAAU;IAAEc,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEL,MAAA,CAAAM,MAAA,IAAAN,MAAA,CAAAM,MAAA,IAAAD,IAAA,CAAM;KAJ1C,IAAAE,UAAA,IAYMjB,mBAAA,CAA0B,YAAAkB,gBAAA,CAAnBR,MAAA,CAAAS,KAAK,CAACC,KAAK,kBAClBpB,mBAAA,CASM,OATNqB,UASM,I,kBARJhB,mBAAA,CAOMiB,SAAA,QArBdC,WAAA,CAe0Bb,MAAA,CAAAc,MAAM,EAAfC,KAAK;yBADdpB,mBAAA,CAOM;MALHqB,GAAG,EAAED,KAAK,CAACE,EAAE;MACd5B,KAAK,EAAC,YAAY;MACjBc,OAAK,EAAAe,MAAA,IAAElB,MAAA,CAAAmB,SAAS,CAACJ,K