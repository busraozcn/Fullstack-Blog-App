{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport md5 from \"md5\";\nimport { useRouter } from \"vue-router\";\nexport default {\n  name: \"SidebarComponent\",\n  props: {\n    selectedUser: {\n      type: Object,\n      default: null\n    }\n  },\n  setup() {\n    const router = useRouter();\n    const navigateTo = path => {\n      router.push(path);\n    };\n    const getGravatarUrl = email => {\n      const hash = md5(email.trim().toLowerCase());\n      return \"https://www.gravatar.com/avatar/${hash}\";\n    };\n    return {\n      navigateTo,\n      getGravatarUrl\n    };\n  }\n};","map":{"version":3,"names":["md5","useRouter","name","props","selectedUser","type","Object","default","setup","router","navigateTo","path","push","getGravatarUrl","email","hash","trim","toLowerCase"],"sources":["C:\\Users\\Excalibur\\Desktop\\vue-task\\src\\components\\sidebar\\SidebarComponent.vue"],"sourcesContent":["<template>\r\n  <div class=\"sidebar\">\r\n    <div class=\"profile\" v-if=\"selectedUser\">\r\n      <img\r\n        :src=\"getGravatarUrl('selectedUser.email')\"\r\n        alt=\"Profile Picture\"\r\n        class=\"profile-img\"\r\n      />\r\n      <div class=\"profile-info\">\r\n        <h2>{{ selectedUser.name }}</h2>\r\n        <p>{{ selectedUser.email }}</p>\r\n        <p>{{ selectedUser.phone }}</p>\r\n      </div>\r\n    </div>\r\n    <ul class=\"menu\">\r\n      <li class=\"menu-item\">\r\n        <a @click=\"navigateTo('/todos')\" class=\"menu-link\">Todos</a>\r\n      </li>\r\n      <li class=\"menu-item\">\r\n        <a @click=\"navigateTo('/posts')\" class=\"menu-link\">Posts</a>\r\n      </li>\r\n      <li class=\"menu-item\">\r\n        <a @click=\"navigateTo('/albums')\" class=\"menu-link\">Albums</a>\r\n      </li>\r\n    </ul>\r\n    <div class=\"footer\">\r\n      <img\r\n        :src=\"require('@/assets/n2mobil.png')\"\r\n        alt=\"logo\"\r\n        class=\"footer-logo\"\r\n      />\r\n      <span>N2Mobil</span>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport md5 from \"md5\";\r\nimport { useRouter } from \"vue-router\";\r\n\r\nexport default {\r\n  name: \"SidebarComponent\",\r\n  props: {\r\n    selectedUser: {\r\n      type: Object,\r\n      default: null,\r\n    },\r\n  },\r\n  setup() {\r\n    const router = useRouter();\r\n\r\n    const navigateTo = (path) => {\r\n      router.push(path);\r\n    };\r\n    const getGravatarUrl = (email) => {\r\n      const hash = md5(email.trim().toLowerCase());\r\n      return \"https://www.gravatar.com/avatar/${hash}\";\r\n    };\r\n\r\n    return {\r\n      navigateTo,\r\n      getGravatarUrl,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n:root {\r\n  --sidebar-bg-color: #f5f5f5;\r\n  --sidebar-item-hover: #ffffff;\r\n  --font-family: \"Poppins\", sans-serif;\r\n  --font-size: 18px;\r\n  --font-weight: 400;\r\n  --line-height: 27px;\r\n  --text-color: #00000073;\r\n}\r\n\r\n.sidebar {\r\n  color: var(--text-color);\r\n  background-color: var(--sidebar-bg-color);\r\n  position: fixed;\r\n  z-index: 1;\r\n  top: 0;\r\n  left: 0;\r\n  bottom: 0;\r\n  display: flex;\r\n  flex-direction: column;\r\n  width: 260px;\r\n  height: 100%;\r\n  padding: 32px 0;\r\n  gap: 0;\r\n  border-right: 1px solid #d8d9dd;\r\n}\r\n\r\n.profile {\r\n  display: flex;\r\n  align-items: center;\r\n  margin-bottom: 1em;\r\n}\r\n\r\n.profile-img {\r\n  width: 48px;\r\n  height: 48px;\r\n  border-radius: 50%;\r\n  margin-right: 10px;\r\n}\r\n\r\n.profile-info h2 {\r\n  font-family: Poppins;\r\n  font-size: 18px;\r\n  font-weight: 500;\r\n  line-height: 27px;\r\n  text-align: left;\r\n}\r\n\r\n.profile-info p {\r\n  font-family: Josefin Sans;\r\n  font-size: 14px;\r\n  font-weight: 300;\r\n  line-height: 14px;\r\n  text-align: left;\r\n}\r\n\r\n.menu {\r\n  list-style: none;\r\n  padding: 0;\r\n  margin: 0;\r\n  flex-grow: 1;\r\n}\r\n\r\n.menu-item {\r\n  margin-bottom: 0.5em;\r\n}\r\n\r\n.menu-link {\r\n  font-family: var(--font-family);\r\n  