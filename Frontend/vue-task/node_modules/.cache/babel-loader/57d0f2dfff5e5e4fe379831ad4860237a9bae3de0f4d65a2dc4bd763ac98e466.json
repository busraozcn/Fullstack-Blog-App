{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ModalPhotoComponent from \"./ModalPhotoComponent.vue\";\nimport SidebarComponent from \"./sidebar/SidebarComponent.vue\";\nimport { ref, onMounted } from \"vue\";\nimport { useRouter, useRoute } from \"vue-router\";\nimport axios from \"axios\";\nexport default {\n  name: \"AlbumDetailPage\",\n  components: {\n    SidebarComponent,\n    ModalPhotoComponent\n  },\n  setup() {\n    const router = useRouter();\n    const route = useRoute();\n    const album = ref({});\n    const photos = ref([]);\n    const showModal = ref(false);\n    const selectedPhoto = ref(null);\n    const goBack = () => {\n      router.push(\"/albums\");\n    };\n    const fetchAlbumAndPhotos = async albumId => {\n      try {\n        // Albüm bilgisi\n        const albumResponse = await axios.get(`https://jsonplaceholder.typicode.com/albums/${albumId}`);\n        album.value = albumResponse.data;\n\n        // Albüm fotoğrafları\n        const photosResponse = await axios.get(`https://jsonplaceholder.typicode.com/photos?albumId=${albumId}`);\n        photos.value = photosResponse.data;\n      } catch (error) {\n        console.error(\"Error fetching album and photos:\", error);\n      }\n    };\n    const openModal = photo => {\n      selectedPhoto.value = photo;\n      showModal.value = true;\n    };\n    const closeModal = () => {\n      showModal.value = false;\n      selectedPhoto.value = null;\n    };\n    onMounted(() => {\n      const albumId = route.params.albumId;\n      fetchAlbumAndPhotos(albumId);\n    });\n    return {\n      album,\n      photos,\n      showModal,\n      selectedPhoto,\n      goBack,\n      openModal,\n      closeModal\n    };\n  }\n};","map":{"version":3,"names":["ModalPhotoComponent","SidebarComponent","ref","onMounted","useRouter","useRoute","axios","name","components","setup","router","route","album","photos","showModal","selectedPhoto","goBack","push","fetchAlbumAndPhotos","albumId","albumResponse","get","value","data","photosResponse","error","console","openModal","photo","closeModal","params"],"sources":["C:\\Users\\Excalibur\\Desktop\\vue-task\\src\\components\\AlbumDetailPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"album-detail-page\">\r\n    <SidebarComponent :user=\"selectedUser\" />\r\n    <div class=\"content\">\r\n      <button @click=\"goBack\">Go Back</button>\r\n      <h1>{{ album.title }}</h1>\r\n      <div class=\"photos-list\">\r\n        <div\r\n          v-for=\"photo in photos\"\r\n          :key=\"photo.id\"\r\n          class=\"photo-card\"\r\n          @click=\"openModal(photo)\"\r\n        >\r\n          <img :src=\"photo.thumbnailUrl\" alt=\"Photo\" />\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <ModalPhotoComponent\r\n      v-if=\"showModal\"\r\n      :show=\"showModal\"\r\n      :photo=\"selectedPhoto\"\r\n      @close=\"closeModal\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ModalPhotoComponent from \"./ModalPhotoComponent.vue\";\r\nimport SidebarComponent from \"./sidebar/SidebarComponent.vue\";\r\nimport { ref, onMounted } from \"vue\";\r\nimport { useRouter, useRoute } from \"vue-router\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"AlbumDetailPage\",\r\n  components: {\r\n    SidebarComponent,\r\n    ModalPhotoComponent,\r\n  },\r\n  setup() {\r\n    const router = useRouter();\r\n    const route = useRoute();\r\n    const album = ref({});\r\n    const photos = ref([]);\r\n    const showModal = ref(false);\r\n    const selectedPhoto = ref(null);\r\n\r\n    const goBack = () => {\r\n      router.push(\"/albums\");\r\n    };\r\n\r\n    const fetchAlbumAndPhotos = async (albumId) => {\r\n      try {\r\n        // Albüm bilgisi\r\n        const albumResponse = await axios.get(\r\n          `https://jsonplaceholder.typicode.com/albums/${albumId}`\r\n        );\r\n        album.value = albumResponse.data;\r\n\r\n        // Albüm fotoğrafları\r\n        const photosResponse = await axios.get(\r\n          `https://jsonplaceholder.typicode.com/photos?albumId=${albumId}`\r\n        );\r\n        photos.value = photosResponse.data;\r\n      } catch (error) {\r\n        console.error(\"Error fetching album and photos:\", error);\r\n      }\r\n    };\r\n\r\n    const openModal = (photo) => {\r\n      selectedPhoto.value = photo;\r\n      showModal.value = true;\r\n    };\r\n\r\n    const closeModal = () => {\r\n      showModal.value = false;\r\n      selectedPhoto.value = null;\r\n    };\r\n\r\n    onMounted(() => {\r\n      const albumId = route.params.albumId;\r\n      fetchAlbumAndPhotos(albumId);\r\n    });\r\n\r\n    return {\r\n      album,\r\n      photos,\r\n      showModal,\r\n      selectedPhoto,\r\n      goBack,\r\n      openModal,\r\n      closeModal,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.album-detail-page {\r\n  display: flex;\r\n}\r\n\r\n.content {\r\n  margin-left: 260px;\r\n  padding: 20px;\r\n}\r\n\r\n.photos-list {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 20px;\r\n}\r\n\r\n.photo-card {\r\n  width: 200px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n  cursor: pointer;\r\n}\r\n\r\n.photo-card img {\r\n  width: 100%;\r\n  height: 150px;\r\n  object-fit: cover;\r\n}\r\n</style>\r\n"],"mappings":";AA2BA,OAAOA,mBAAkB,MAAO,2BAA2B;AAC3D,OAAOC,gBAAe,MAAO,gCAAgC;AAC7D,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,SAAS,EAAEC,QAAO,QAAS,YAAY;AAChD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACVP,gBAAgB;IAChBD;EACF,CAAC;EACDS,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIN,SAAS,CAAC,CAAC;IAC1B,MAAMO,KAAI,GAAIN,QAAQ,CAAC,CAAC;IACxB,MAAMO,KAAI,GAAIV,GAAG,CAAC,CAAC,CAAC,CAAC;IACrB,MAAMW,MAAK,GAAIX,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMY,SAAQ,GAAIZ,GAAG,CAAC,KAAK,CAAC;IAC5B,MAAMa,aAAY,GAAIb,GAAG,CAAC,IAAI,CAAC;IAE/B,MAAMc,MAAK,GAAIA,CAAA,KAAM;MACnBN,MAAM,CAACO,IAAI,CAAC,SAAS,CAAC;IACxB,CAAC;IAED,MAAMC,mBAAkB,GAAI,MAAOC,OAAO,IAAK;MAC7C,IAAI;QACF;QACA,