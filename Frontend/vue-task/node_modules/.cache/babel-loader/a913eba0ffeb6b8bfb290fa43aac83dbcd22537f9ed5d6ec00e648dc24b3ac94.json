{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport SidebarComponent from \"./sidebar/SidebarComponent.vue\";\nimport { useRouter } from \"vue-router\";\nimport axios from \"axios\";\nexport default {\n  name: \"TodosPage\",\n  components: {\n    SidebarComponent\n  },\n  data() {\n    return {\n      todos: [{\n        id: 1,\n        text: \"Do homework\",\n        completed: false\n      }, {\n        id: 2,\n        text: \"Read a book\",\n        completed: false\n      }, {\n        id: 3,\n        text: \"Write some code\",\n        completed: false\n      }],\n      selectedUser: null\n    };\n  },\n  setup() {\n    const router = useRouter();\n    const goBack = () => {\n      router.push(\"/\");\n    };\n    return {\n      goBack\n    };\n  },\n  created() {\n    const userId = this.$route.query.userId;\n    if (userId) {\n      axios.get(`https://jsonplaceholder.typicode.com/users/${userId}`).then(response => {\n        this.selectedUser = response.data;\n      }).catch(error => {\n        console.error(\"Error fetching user:\", error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["SidebarComponent","useRouter","axios","name","components","data","todos","id","text","completed","selectedUser","setup","router","goBack","push","created","userId","$route","query","get","then","response","catch","error","console"],"sources":["C:\\Users\\Excalibur\\Desktop\\vue-task\\src\\components\\TodosPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"todos-page\">\r\n    <SidebarComponent :selectedUser=\"selectedUser\" />\r\n    <button @click=\"goBack\">Geri</button>\r\n    <h1>Todo List</h1>\r\n    <ul>\r\n      <li v-for=\"todo in todos\" :key=\"todo.id\">\r\n        <input type=\"checkbox\" v-model=\"todo.completed\" />\r\n        <span :class=\"{ completed: todo.completed }\">{{ todo.text }}</span>\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SidebarComponent from \"./sidebar/SidebarComponent.vue\";\r\nimport { useRouter } from \"vue-router\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"TodosPage\",\r\n  components: {\r\n    SidebarComponent,\r\n  },\r\n  data() {\r\n    return {\r\n      todos: [\r\n        { id: 1, text: \"Do homework\", completed: false },\r\n        { id: 2, text: \"Read a book\", completed: false },\r\n        { id: 3, text: \"Write some code\", completed: false },\r\n      ],\r\n      selectedUser: null,\r\n    };\r\n  },\r\n  setup() {\r\n    const router = useRouter();\r\n\r\n    const goBack = () => {\r\n      router.push(\"/\");\r\n    };\r\n\r\n    return {\r\n      goBack,\r\n    };\r\n  },\r\n  created() {\r\n    const userId = this.$route.query.userId;\r\n    if (userId) {\r\n      axios\r\n        .get(`https://jsonplaceholder.typicode.com/users/${userId}`)\r\n        .then((response) => {\r\n          this.selectedUser = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Error fetching user:\", error);\r\n        });\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.geritusu {\r\n  margin: 10px;\r\n  padding: 8px 16px;\r\n  background-color: #007bff;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n.geritusu:hover {\r\n  background-color: #0056b3;\r\n}\r\n</style>\r\n"],"mappings":";AAeA,OAAOA,gBAAe,MAAO,gCAAgC;AAC7D,SAASC,SAAQ,QAAS,YAAY;AACtC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,aAAa;QAAEC,SAAS,EAAE;MAAM,CAAC,EAChD;QAAEF,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,aAAa;QAAEC,SAAS,EAAE;MAAM,CAAC,EAChD;QAAEF,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,iBAAiB;QAAEC,SAAS,EAAE;MAAM,CAAC,CACrD;MACDC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIX,SAAS,CAAC,CAAC;IAE1B,MAAMY,MAAK,GAAIA,CAAA,KAAM;MACnBD,MAAM,CAACE,IAAI,CAAC,GAAG,CAAC;IAClB,CAAC;IAED,OAAO;MACLD;IACF,CAAC;EACH,CA