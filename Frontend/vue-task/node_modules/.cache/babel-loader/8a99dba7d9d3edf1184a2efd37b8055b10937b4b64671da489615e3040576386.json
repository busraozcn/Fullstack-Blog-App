{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted } from \"vue\";\nimport { useRouter, useRoute } from \"vue-router\";\nimport axios from \"axios\";\nimport SidebarComponent from \"./sidebar/SidebarComponent.vue\";\nexport default {\n  name: \"AlbumsPage\",\n  components: {\n    SidebarComponent\n  },\n  setup() {\n    const router = useRouter();\n    const route = useRoute();\n    const albums = ref([]);\n    const selectedUser = ref(null);\n    const fetchUserAndAlbums = async () => {\n      const userId = route.query.userId;\n      if (userId) {\n        try {\n          const userResponse = await axios.get(`https://jsonplaceholder.typicode.com/users/${userId}`);\n          selectedUser.value = userResponse.data;\n          const albumsResponse = await axios.get(`https://jsonplaceholder.typicode.com/albums?userId=${userId}`);\n          albums.value = albumsResponse.data.map(album => ({\n            ...album,\n            thumbnailUrl: `https://via.placeholder.com/150/0000FF/808080?text=${album.title}`\n          }));\n        } catch (error) {\n          console.error(\"Error fetching data:\", error);\n        }\n      }\n    };\n    const goToHome = () => {\n      router.push(\"/\");\n    };\n    const goToAlbum = albumId => {\n      router.push(`/albums/${albumId}`);\n    };\n    onMounted(fetchUserAndAlbums);\n    return {\n      albums,\n      selectedUser,\n      goToHome,\n      goToAlbum\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","useRouter","useRoute","axios","SidebarComponent","name","components","setup","router","route","albums","selectedUser","fetchUserAndAlbums","userId","query","userResponse","get","value","data","albumsResponse","map","album","thumbnailUrl","title","error","console","goToHome","push","goToAlbum","albumId"],"sources":["C:\\Users\\Excalibur\\Desktop\\vue-task\\src\\components\\AlbumsPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"albums-page\">\r\n    <SidebarComponent :user=\"selectedUser\" />\r\n    <div class=\"content\">\r\n      <button class=\"geritusu\" @click=\"goToHome\">Geri</button>\r\n      <h1>Albums</h1>\r\n      <div class=\"albums-list\">\r\n        <div\r\n          v-for=\"album in albums\"\r\n          :key=\"album.id\"\r\n          class=\"album-card\"\r\n          @click=\"goToAlbum(album.id)\"\r\n        >\r\n          <img :src=\"album.cover\" alt=\"Album cover\" />\r\n          <h3>{{ album.title }}</h3>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted } from \"vue\";\r\nimport { useRouter, useRoute } from \"vue-router\";\r\nimport axios from \"axios\";\r\nimport SidebarComponent from \"./sidebar/SidebarComponent.vue\";\r\n\r\nexport default {\r\n  name: \"AlbumsPage\",\r\n  components: {\r\n    SidebarComponent,\r\n  },\r\n  setup() {\r\n    const router = useRouter();\r\n    const route = useRoute();\r\n    const albums = ref([]);\r\n    const selectedUser = ref(null);\r\n\r\n    const fetchUserAndAlbums = async () => {\r\n      const userId = route.query.userId;\r\n      if (userId) {\r\n        try {\r\n          const userResponse = await axios.get(\r\n            `https://jsonplaceholder.typicode.com/users/${userId}`\r\n          );\r\n          selectedUser.value = userResponse.data;\r\n\r\n          const albumsResponse = await axios.get(\r\n            `https://jsonplaceholder.typicode.com/albums?userId=${userId}`\r\n          );\r\n          albums.value = albumsResponse.data.map((album) => ({\r\n            ...album,\r\n            thumbnailUrl: `https://via.placeholder.com/150/0000FF/808080?text=${album.title}`,\r\n          }));\r\n        } catch (error) {\r\n          console.error(\"Error fetching data:\", error);\r\n        }\r\n      }\r\n    };\r\n\r\n    const goToHome = () => {\r\n      router.push(\"/\");\r\n    };\r\n\r\n    const goToAlbum = (albumId) => {\r\n      router.push(`/albums/${albumId}`);\r\n    };\r\n\r\n    onMounted(fetchUserAndAlbums);\r\n\r\n    return {\r\n      albums,\r\n      selectedUser,\r\n      goToHome,\r\n      goToAlbum,\r\n    };\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.albums-page {\r\n  display: flex;\r\n}\r\n\r\n.content {\r\n  margin-left: 260px;\r\n  padding: 20px;\r\n}\r\n\r\n.albums-list {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 20px;\r\n}\r\n\r\n.album-card {\r\n  width: 200px;\r\n  cursor: pointer;\r\n  border: 1px solid #ccc;\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n  text-align: center;\r\n}\r\n\r\n.album-card img {\r\n  width: 100%;\r\n  height: 150px;\r\n  object-fit: cover;\r\n}\r\n\r\n.album-card h3 {\r\n  margin: 10px 0;\r\n  font-size: 18px;\r\n}\r\n</style>\r\n"],"mappings":";AAsBA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,SAAS,EAAEC,QAAO,QAAS,YAAY;AAChD,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,gBAAe,MAAO,gCAAgC;AAE7D,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIP,SAAS,CAAC,CAAC;IAC1B,MAAMQ,KAAI,GAAIP,QAAQ,CAAC,CAAC;IACxB,MAAMQ,MAAK,GAAIX,GAAG,CAAC,EAAE,CAAC;IACtB,MAAMY,YAAW,GAAIZ,GAAG,CAAC,IAAI,CAAC;IAE9B,MAAMa,kBAAiB,GAAI,MAAAA,CAAA,KAAY;MACrC,MAAMC,MAAK,GAAIJ,KAAK,CAACK,KAAK,CAACD,MAAM;MACjC,IAAIA,MAAM,EAAE;QACV,IAAI;UACF,MAAME,YAAW,GAAI,MAAMZ,KAAK,CAACa,GAAG,CAClC,8CAA8CH,MAAM,EACtD,CAAC;UACDF,YAAY,CAACM,KAAI,GAAIF,YAAY,CAACG,IAAI;UAEtC,MAAMC,cAAa,GAAI,MAAMhB,KAAK,CAACa,GAAG,CACpC,sDAAsDH,MAAM,EAC9D,CAAC;UACDH,MAAM,CAACO,KAAI,GAAIE,cAAc,CAACD,IAAI,CAACE,GAAG,CAAEC,KAAK,KAAM;YACjD,GAAGA,KAAK;YACRC,YAAY,EAAE,sDAAsDD,KAAK,CAACE,KAAK;UACjF,CAAC,CAAC,CAAC;QACL,EAAE,OAAOC,KAAK,EAAE;UACdC,OAAO,CAACD,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;QAC9C;MACF;IACF,CAAC;IAED,MAAME,QAAO,GAAIA,CAAA,KAAM;MACrBlB,MAAM,CAACmB,IAAI,CAAC,GAAG,CAAC;IAClB,CAAC;IAED,MAAMC,SAAQ,GAAKC,OAAO,IAAK;MAC7BrB,MAAM,CAACmB,IAAI,CAAC,WAAWE,OAAO,EAAE,CAAC;IACnC,CAAC;IAED7B,SAAS,CAACY,kBAAkB,CAAC;IAE7B,OAAO;MACLF,MAAM;MACNC,YAAY;MACZe,QAAQ;MACRE;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata