{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, onMounted } from \"vue\";\nimport { useRouter, useRoute } from \"vue-router\";\nimport axios from \"axios\";\nimport ModalPostsComponent from \"./ModalPostsComponent.vue\";\nimport SidebarComponent from \"./sidebar/SidebarComponent.vue\";\nexport default {\n  name: \"PostsPage\",\n  components: {\n    SidebarComponent,\n    ModalPostsComponent\n  },\n  setup() {\n    const router = useRouter();\n    const route = useRoute();\n    const posts = ref([]);\n    const showModal = ref(false);\n    const selectedPost = ref({});\n    const selectedUser = ref(null);\n    const fetchPostsAndUser = async () => {\n      const userId = route.query.userId;\n      if (userId) {\n        try {\n          const userResponse = await axios.get(`https://jsonplaceholder.typicode.com/users/${userId}`);\n          selectedUser.value = userResponse.data;\n          const postsResponse = await axios.get(`https://jsonplaceholder.typicode.com/posts?userId=${userId}`);\n          posts.value = postsResponse.data;\n        } catch (error) {\n          console.error(\"Error fetching data:\", error);\n        }\n      }\n    };\n    const openPost = post => {\n      selectedPost.value = post;\n      showModal.value = true;\n    };\n    const closeModal = () => {\n      showModal.value = false;\n    };\n    const goBack = () => {\n      router.push(\"/\");\n    };\n    onMounted(fetchPostsAndUser);\n    return {\n      posts,\n      showModal,\n      selectedPost,\n      selectedUser,\n      openPost,\n      closeModal,\n      goBack\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","useRouter","useRoute","axios","ModalPostsComponent","SidebarComponent","name","components","setup","router","route","posts","showModal","selectedPost","selectedUser","fetchPostsAndUser","userId","query","userResponse","get","value","data","postsResponse","error","console","openPost","post","closeModal","goBack","push"],"sources":["C:\\Users\\Excalibur\\Desktop\\vue-task\\src\\components\\PostsPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"posts-page\">\r\n    <SidebarComponent :user=\"selectedUser\" />\r\n    <button @click=\"goBack\">Geri</button>\r\n    <div class=\"content\">\r\n      <h1>Posts Page</h1>\r\n      <ul>\r\n        <li v-for=\"post in posts\" :key=\"post.id\">\r\n          <a href=\"#\" @click.prevent=\"openPost(post)\">{{ post.title }}</a>\r\n        </li>\r\n      </ul>\r\n      <ModalPostsComponent :visible=\"showModal\" @close=\"closeModal\">\r\n        <template #header>\r\n          <h2>{{ selectedPost.title }}</h2>\r\n        </template>\r\n        <template #body>\r\n          <p>{{ selectedPost.body }}</p>\r\n        </template>\r\n        <template #footer>\r\n          <button @click=\"closeModal\">Close</button>\r\n        </template>\r\n      </ModalPostsComponent>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted } from \"vue\";\r\nimport { useRouter, useRoute } from \"vue-router\";\r\nimport axios from \"axios\";\r\nimport ModalPostsComponent from \"./ModalPostsComponent.vue\";\r\nimport SidebarComponent from \"./sidebar/SidebarComponent.vue\";\r\n\r\nexport default {\r\n  name: \"PostsPage\",\r\n  components: {\r\n    SidebarComponent,\r\n    ModalPostsComponent,\r\n  },\r\n  setup() {\r\n    const router = useRouter();\r\n    const route = useRoute();\r\n    const posts = ref([]);\r\n    const showModal = ref(false);\r\n    const selectedPost = ref({});\r\n    const selectedUser = ref(null);\r\n\r\n    const fetchPostsAndUser = async () => {\r\n      const userId = route.query.userId;\r\n      if (userId) {\r\n        try {\r\n          const userResponse = await axios.get(\r\n            `https://jsonplaceholder.typicode.com/users/${userId}`\r\n          );\r\n          selectedUser.value = userResponse.data;\r\n\r\n          const postsResponse = await axios.get(\r\n            `https://jsonplaceholder.typicode.com/posts?userId=${userId}`\r\n          );\r\n          posts.value = postsResponse.data;\r\n        } catch (error) {\r\n          console.error(\"Error fetching data:\", error);\r\n        }\r\n      }\r\n    };\r\n\r\n    const openPost = (post) => {\r\n      selectedPost.value = post;\r\n      showModal.value = true;\r\n    };\r\n\r\n    const closeModal = () => {\r\n      showModal.value = false;\r\n    };\r\n\r\n    const goBack = () => {\r\n      router.push(\"/\");\r\n    };\r\n\r\n    onMounted(fetchPostsAndUser);\r\n\r\n    return {\r\n      posts,\r\n      showModal,\r\n      selectedPost,\r\n      selectedUser,\r\n      openPost,\r\n      closeModal,\r\n      goBack,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.posts-page {\r\n  display: flex;\r\n}\r\n\r\n.content {\r\n  margin-left: 260px;\r\n  padding: 20px;\r\n}\r\n\r\nul {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\n\r\nli {\r\n  margin-bottom: 10px;\r\n}\r\n\r\na {\r\n  text-decoration: none;\r\n  color: #007bff;\r\n  cursor: pointer;\r\n}\r\n\r\na:hover {\r\n  text-decoration: underline;\r\n}\r\n</style>\r\n"],"mappings":";AA2BA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,SAAS,EAAEC,QAAO,QAAS,YAAY;AAChD,OAAOC,KAAI,MAAO,OAAO;AACzB,OAAOC,mBAAkB,MAAO,2BAA2B;AAC3D,OAAOC,gBAAe,MAAO,gCAAgC;AAE7D,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVF,gBAAgB;IAChBD;EACF,CAAC;EACDI,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIR,SAAS,CAAC,CAAC;IAC1B,MAAMS,KAAI,GAAIR,QAAQ,CAAC,CAAC;IACxB,MAAMS,KAAI,GAAIZ,GAAG,CAAC,EAAE,CAAC;IACrB,MAAMa,SAAQ,GAAIb,GAAG,CAAC,KAAK,CAAC;IAC5B,MAAMc,YAAW,GAAId,GAAG,CAAC,CAAC,CAAC,CAAC;IAC5B,MAAMe,YAAW,GAAIf,GAAG,CAAC,IAAI,CAAC;IAE9B,MAAMgB,iBAAgB,GAAI,MAAAA,CAAA,KAAY;MACpC,MAAMC,MAAK,GAAIN,KAAK,CAACO,KAAK,CAACD,MAAM;MACjC,IAAIA,MAAM,EAAE;QACV,IAAI;UACF,MAAME,YAAW,GAAI,MAAMf,KAAK,CAACgB,GAAG,CAClC,8CAA8CH,MAAM,EACtD,CAAC;UACDF,YAAY,CAACM,KAAI,GAAIF,YAAY,CAACG,IAAI;UAEtC,MAAMC,aAAY,GAAI,MAAMnB,KAAK,CAACgB,GAAG,CACnC,qDAAqDH,MAAM,EAC7D,CAAC;UACDL,KAAK,CAACS,KAAI,GAAIE,aAA