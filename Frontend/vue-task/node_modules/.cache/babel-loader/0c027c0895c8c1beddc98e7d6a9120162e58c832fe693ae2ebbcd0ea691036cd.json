hotos: [\r\n            { id: 1, src: \"./src/assets/logo.png\" },\r\n            { id: 2, src: \"./src/assets/logo.png\" },\r\n          ],\r\n        },\r\n        {\r\n          id: 3,\r\n          title: \"Friends\",\r\n          photos: [\r\n            { id: 1, src: \"./src/assets/logo.png\" },\r\n            { id: 2, src: \"./src/assets/logo.png\" },\r\n          ],\r\n        },\r\n        // Other albums...\r\n      ];\r\n      return albums.find((album) => album.id === parseInt(id));\r\n    },\r\n    openModal(photo) {\r\n      this.selectedPhoto = photo;\r\n      this.showModal = true;\r\n    },\r\n    closeModal() {\r\n      this.showModal = false;\r\n      this.selectedPhoto = null;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.album-detail-page {\r\n  display: flex;\r\n}\r\n\r\n.content {\r\n  margin-left: 260px;\r\n  padding: 20px;\r\n}\r\n\r\n.photos-list {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 20px;\r\n}\r\n\r\n.photo-card {\r\n  width: 200px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n  cursor: pointer;\r\n}\r\n\r\n.photo-card img {\r\n  width: 100%;\r\n  height: 150px;\r\n  object-fit: cover;\r\n}\r\n</style>\r\n"],"mappings":";AA2BA,OAAOA,mBAAkB,MAAO,2BAA2B;AAC3D,OAAOC,gBAAe,MAAO,gCAAgC;AAE7D,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACVF,gBAAgB;IAChBD;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;QACLC,KAAK,EAAE,EAAE;QACTC,MAAM,EAAE;MACV,CAAC;MACDC,SAAS,EAAE,KAAK;MAChBC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACDC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIA,CAAA,KAAM;MACnBC,MAAM,CAACC,IAAI,CAAC,SAAS,CAAC;IACxB,CAAC;IAED,OAAO;MACLF;IACF,CAAC;EACH,CAAC;EACDG,OAAOA,CAAA,EAAG;IACR,MAAMC,OAAM,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,OAAO;IAC1C,IAAI,CAACV,KAAI,GAAI,IAAI,CAACa,YAAY,CAACH,OAAO,CAAC;EACzC,CAAC;EACDI,OAAO,EAAE;IACPD,YAAYA,CAACE,EAAE,EAAE;MACf,MAAMC,MAAK,GAAI,CACb;QACED,EAAE,EAAE,CAAC;QACLd,KAAK,EAAE,UAAU;QACjBC,MAAM,EAAE,CACN;UAAEa,EAAE,EAAE,CAAC;UAAEE,GAAG,EAAE;QAAwB,CAAC,EACvC;