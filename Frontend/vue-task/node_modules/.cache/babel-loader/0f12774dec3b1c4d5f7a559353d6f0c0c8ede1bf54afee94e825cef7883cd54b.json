te>\r\n  <div class=\"album-detail-page\">\r\n    <SidebarComponent />\r\n    <div class=\"content\">\r\n      <button @click=\"goBack\">Go Back</button>\r\n      <h1>{{ album.title }}</h1>\r\n      <div class=\"photos-list\">\r\n        <div v-for=\"photo in album.photos\" :key=\"photo.id\" class=\"photo-card\">\r\n          <img :src=\"photo.src\" alt=\"Photo\" />\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SidebarComponent from \"./sidebar/SidebarComponent.vue\";\r\nimport { useRouter, useRoute } from \"vue-router\";\r\n\r\nexport default {\r\n  name: \"AlbumDetailPage\",\r\n  components: {\r\n    SidebarComponent,\r\n  },\r\n  data() {\r\n    return {\r\n      album: {\r\n        title: \"\",\r\n        photos: [],\r\n      },\r\n    };\r\n  },\r\n  setup() {\r\n    const router = useRouter();\r\n    const route = useRoute(); // eslint-disable-line no-unused-vars\r\n\r\n    const goBack = () => {\r\n      router.push(\"/albums\");\r\n    };\r\n\r\n    return {\r\n      goBack,\r\n    };\r\n  },\r\n  mounted() {\r\n    const albumId = this.$route.params.albumId;\r\n    this.album = this.getAlbumById(albumId);\r\n  },\r\n  methods: {\r\n    getAlbumById(id) {\r\n      // Gerçek verileri fetch edeceğiniz yer\r\n      const albums = [\r\n        {\r\n          id: 1,\r\n          title: \"Vacation\",\r\n          photos: [\r\n            { id: 1, src: \"./src/assets/logo.png\" },\r\n            { id: 2, src: \"./src/assets/logo.png\" },\r\n          ],\r\n        },\r\n        {\r\n          id: 2,\r\n          title: \"Family\",\r\n          photos: [\r\n            { id: 1, src: \"./src/assets/logo.png\" },\r\n            { id: 2, src: \"./src/assets/logo.png\" },\r\n          ],\r\n        },\r\n        {\r\n          id: 3,\r\n          title: \"Friends\",\r\n          photos: [\r\n            { id: 1, src: \"./src/assets/logo.png\" },\r\n            { id: 2, src: \"./src/assets/logo.png\" },\r\n          ],\r\n        },\r\n        // Diğer albümler...\r\n      ];\r\n      return albums.find((album) => album.id === parseInt(id));\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.album-detail-page {\r\n  display: flex;\r\n}\r\n\r\n.content {\r\n  margin-left: 260px;\r\n  padding: 20px;\r\n}\r\n\r\n.photos-list {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 20px;\r\n}\r\n\r\n.photo-card {\r\n  width: 200px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n}\r\n\r\n.photo-card img {\r\n  width: 100%;\r\n  height: 150px;\r\n  object-fit: cover;\r\n}\r\n</style>\r\n"],"mappings":";AAgBA,OAAOA,gBAAe,MAAO,gCAAgC;AAC7D,SAASC,SAAS,EAAEC,QAAO,QAAS,YAAY;AAEhD,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;QACLC,KAAK,EAAE,EAAE;QACTC,MAAM,EAAE;MACV;IACF,CAAC;EACH,CAAC;EACDC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIT,SAAS,CAAC,CAAC;IAC1B,MAAMU,KAAI,GAAIT,QAAQ,CAAC,CAAC,EAAE;;IAE1B,MAAMU,MAAK,GAAIA,CAAA,KAAM;MACnBF,MAAM,CAACG,IAAI,CAAC,SAAS,CAAC;IACxB,CAAC;IAED,OAAO;MACLD;IACF,CAAC;EACH,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR,MAAMC,OAAM,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,OAAO;IAC1C,IAAI,CAACT,KAAI,GAAI,IAAI,CAACY,YAAY,CAACH,OAAO,CAAC;EACzC,CAAC;EACDI,OAAO,EAAE;IACPD,YAAYA,CAACE,EAAE,EAAE;MACf;MACA,MAAMC,MAAK,GAAI,CACb;QACED,EAAE,EAAE,CAAC;QACLb,KAAK,EAAE,UAAU;QACjBC,MAAM,EAAE,CACN;UAAEY,EAAE,EAAE,CAAC;UAAEE,GAAG,EAAE;QAAwB,CAAC,EACvC;UAAEF,EAAE,EAAE,CAAC;UAAEE,GAAG,EAAE;QAAwB,CAAC;MAE3C,CAAC,EACD;QACEF,EAAE,EAAE,CAAC;QACLb,KAAK,EAAE,QAAQ;QACfC,MAAM,EAAE,CACN;UAAEY,EAAE,EAAE,CAAC;UAAEE,GAAG,EAAE;QAAwB,CAAC,EACvC;UAAEF,EAAE,EAAE,CAAC;UAAEE,GAAG,EAAE;QAAwB,CAAC;MAE3C,CAAC,EACD;QACEF,EAAE,EAAE,CAAC;QACLb,KAAK,EAAE,SAAS;QAChBC,MAAM,EAAE,CACN;UAAEY,EAAE,EAAE,CAAC;UAAEE,GAAG,EAAE;QAAwB,CAAC,EACvC;UAAEF,EAAE,EAAE,CAAC;UAAEE,GAAG,EAAE;QAAwB,CAAC;MAE3C;MACA;MAAA,CACD;MACD,OAAOD,MAAM,CAACE,IAAI,CAAEjB,KAAK,IAAKA,KAAK,CAACc,EAAC,KAAMI,QAAQ,CAACJ,EAAE,CAAC,CAAC;IAC1D;EACF;AACF,CAAC","ignoreList":[]}