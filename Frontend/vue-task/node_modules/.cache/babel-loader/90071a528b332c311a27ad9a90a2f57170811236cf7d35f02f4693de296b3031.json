>\r\n          <div class=\"albums-list\">\r\n            <div\r\n              v-for=\"album in albums\"\r\n              :key=\"album.id\"\r\n              class=\"album-card\"\r\n              @click=\"goToAlbum(album.id)\"\r\n            >\r\n              <img :src=\"album.cover\" alt=\"Album cover\" />\r\n              <h3>{{ album.title }}</h3>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </template>\r\n\r\n    <script>\r\n      import SidebarComponent from \"./sidebar/SidebarComponent.vue\";\r\n      import { useRouter } from \"vue-router\";\r\n\r\n      export default {\r\n        name: \"AlbumsPage\",\r\n        components: {\r\n          SidebarComponent,\r\n        },\r\n        setup() {\r\n          const router = useRouter();\r\n\r\n          const goToHome = () => {\r\n            router.push(\"/\");\r\n          };\r\n\r\n          const goToAlbum = (albumId) => {\r\n            router.push(`/albums/${albumId}`);\r\n          };\r\n\r\n          return {\r\n            goToHome,\r\n            goToAlbum,\r\n            albums: [\r\n              {\r\n                id: 1,\r\n                title: \"Non esse culpa molestiae omnis sed ol...\",\r\n                cover: \"./src/assets/logo.png\",\r\n              },\r\n              {\r\n                id: 2,\r\n                title: \"Non esse culpa molestiae omnis sed ol...\",\r\n                cover: \"./src/assets/logo.png\",\r\n              },\r\n              {\r\n                id: 3,\r\n                title: \"Non esse culpa molestiae omnis sed ol...\",\r\n                cover: \"./src/assets/logo.png\",\r\n              },\r\n            ],\r\n          };\r\n        },\r\n      };\r\n    </script>\r\n\r\n    <style>\r\n      .albums-page {\r\n        display: flex;\r\n      }\r\n\r\n      .content {\r\n        margin-left: 260px;\r\n        padding: 20px;\r\n      }\r\n\r\n      .albums-list {\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        gap: 20px;\r\n      }\r\n\r\n      .album-card {\r\n        width: 200px;\r\n        cursor: pointer;\r\n        border: 1px solid #ccc;\r\n        border-radius: 8px;\r\n        overflow: hidden;\r\n        text-align: center;\r\n      }\r\n\r\n      .album-card img {\r\n        width: 100%;\r\n        height: 150px;\r\n        object-fit: cover;\r\n      }\r\n\r\n      .album-card h3 {\r\n        margin: 10px 0;\r\n        font-size: 18px;\r\n      }\r\n    </style>\r\n\r\n    <div class=\"albums-list\">\r\n      <div\r\n        v-for=\"album in albums\"\r\n        :key=\"album.id\"\r\n        class=\"album-card\"\r\n        @click=\"goToAlbum(album.id)\"\r\n      >\r\n        <img src=\"album.cover\" alt=\"Album cover\" />\r\n        <h3>{{ album.title }}</h3>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SidebarComponent from \"./sidebar/SidebarComponent.vue\";\r\nimport { useRouter } from \"vue-router\";\r\n\r\nexport default {\r\n  name: \"AlbumsPage\",\r\n  components: {\r\n    SidebarComponent,\r\n  },\r\n  setup() {\r\n    const router = useRouter();\r\n\r\n    function goToHome() {\r\n      router.push(\"/\");\r\n    }\r\n\r\n    return {\r\n      albums: [\r\n        {\r\n          id: 1,\r\n          title: \"Non esse culpa molestiae omnis sed ol...\",\r\n          cover: \"./src/assets/logo.png\",\r\n        },\r\n        {\r\n          id: 2,\r\n          title: \"Non esse culpa molestiae omnis sed ol...\",\r\n          cover: \"./src/assets/logo.png\",\r\n        },\r\n        {\r\n          id: 3,\r\n          title: \"Non esse culpa molestiae omnis sed ol...\",\r\n          cover: \"./src/assets/logo.png\",\r\n        },\r\n      ],\r\n      goToHome,\r\n    };\r\n  },\r\n  setup() {\r\n    const router = useRouter();\r\n    const goToAlbum = (albumId) => {\r\n      router.push(\"/albums/${albumId}\");\r\n    };\r\n\r\n    return {\r\n      goToAlbum,\r\n    };\r\n  },\r\n};\r\n</script>\r\n"],"mappin