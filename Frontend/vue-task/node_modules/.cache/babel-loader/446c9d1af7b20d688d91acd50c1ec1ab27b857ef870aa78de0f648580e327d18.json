nst photos = ref([]);\r\n    const showModal = ref(false);\r\n    const selectedPhoto = ref(null);\r\n    const selectedUser = ref(null);\r\n\r\n    const goBack = () => {\r\n      const userId = route.query.userId;\r\n      router.push(`/albums?userId=${userId}`);\r\n    };\r\n\r\n    const fetchAlbumAndPhotos = async (albumId) => {\r\n      try {\r\n        const albumResponse = await axios.get(\r\n          `https://jsonplaceholder.typicode.com/albums/${albumId}`\r\n        );\r\n        album.value = albumResponse.data;\r\n\r\n        const photosResponse = await axios.get(\r\n          `https://jsonplaceholder.typicode.com/photos?albumId=${albumId}`\r\n        );\r\n        photos.value = photosResponse.data;\r\n      } catch (error) {\r\n        console.error(\"Error fetching album and photos:\", error);\r\n      }\r\n    };\r\n\r\n    const fetchUser = async (userId) => {\r\n      try {\r\n        const userResponse = await axios.get(\r\n          `https://jsonplaceholder.typicode.com/users/${userId}`\r\n        );\r\n        selectedUser.value = userResponse.data;\r\n        console.log(\"Selected User: \", selectedUser.value); // Konsol log ekleme\r\n      } catch (error) {\r\n        console.error(\"Error fetching user:\", error);\r\n      }\r\n    };\r\n\r\n    const openModal = (photo) => {\r\n      selectedPhoto.value = photo;\r\n      showModal.value = true;\r\n    };\r\n\r\n    const closeModal = () => {\r\n      showModal.value = false;\r\n      selectedPhoto.value = null;\r\n    };\r\n\r\n    onMounted(() => {\r\n      const albumId = route.params.albumId;\r\n      const userId = route.query.userId;\r\n      fetchAlbumAndPhotos(albumId);\r\n      fetchUser(userId);\r\n    });\r\n\r\n    return {\r\n      album,\r\n      photos,\r\n      showModal,\r\n      selectedPhoto,\r\n      goBack,\r\n      openModal,\r\n      closeModal,\r\n      selectedUser,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.album-detail-page {\r\n  display: flex;\r\n}