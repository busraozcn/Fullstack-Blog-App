port default {\r\n  name: \"AlbumsPage\",\r\n  setup() {\r\n    const router = useRouter();\r\n    const route = useRoute();\r\n    const albums = ref([]);\r\n    const selectedUser = ref(null);\r\n\r\n    const fetchUserAndAlbums = async (userId) => {\r\n      try {\r\n        const userResponse = await axios.get(\r\n          `https://jsonplaceholder.typicode.com/users/${userId}`\r\n        );\r\n        selectedUser.value = userResponse.data;\r\n\r\n        const albumsResponse = await axios.get(\r\n          `https://jsonplaceholder.typicode.com/albums?userId=${userId}`\r\n        );\r\n        const albumsData = albumsResponse.data;\r\n\r\n        for (const album of albumsData) {\r\n          const photosResponse = await axios.get(\r\n            `https://jsonplaceholder.typicode.com/photos?albumId=${album.id}`\r\n          );\r\n          album.photos = photosResponse.data;\r\n        }\r\n\r\n        albums.value = albumsData;\r\n      } catch (error) {\r\n        console.error(\"Error fetching data:\", error);\r\n      }\r\n    };\r\n\r\n    const goBack = () => {\r\n      router.push(\"/\");\r\n    };\r\n\r\n    const goToAlbum = (albumId) => {\r\n      const userId = route.query.userId;\r\n      router.push(`/albums/${albumId}?userId=${userId}`);\r\n    };\r\n\r\n    onMounted(() => {\r\n      const userId = route.query.userId;\r\n      if (userId) {\r\n        fetchUserAndAlbums(userId);\r\n      }\r\n    });\r\n\r\n    return {\r\n      albums,\r\n      selectedUser,\r\n      goBack,\r\n      goToAlbum,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.albums-page {\r\n  display: flex;\r\n}\r\n\r\n.content {\r\n  margin-left: 260px;\r\n  padding: 20px;\r\n}\r\n\r\n.albums-list {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 20px;\r\n  margin-top: 300px;\r\n}\r\n\r\n.album-card {\r\n  width: 318px;\r\n  height: 256px;\r\n  cursor: pointer;\r\n  border: 1px solid #ccc;\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n  text-align: center;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\r\n  transition: transform 0.3s, box-shadow 0.3s;\r\n  padding-bottom: 10%;\r\n  padding-left: 1%;\r\n  padding-top: 1%;\r\n  padding-right: 1%;\r\n}\r\n\r\n.album-card:hover {\r\n  transform: translateY(-5px);\r\n  box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n.album-thumbnails {\r\n  display: grid;\r\n  grid-template-columns: repeat(2, 1fr);\r\n  grid-template-rows: repeat(2, 1fr);\r\n  width: 100%;\r\n  height: auto;\r\n}\r\n\r\n.album-thumbnails img {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n\r\n.album-title {\r\n  padding: 10px;\r\n  background: #fff;\r\n}\r\n\r\n.album-title h3 {\r\n  margin: 0;\r\n  font-size: 14px;\r\n  font-weight: 400;\r\n  color: #333;\r\n}\r\n</style>\r\n"],"mappings":";OAKQA,UAAmD;gCAJzDC,mBAAA,CAA+B;EAA1BC,KAAK,EAAC;AAAa;;EACnBA,KAAK,EAAC;AAAS;gCAEhBD,mBAAA,CAIE;EAHAE,GAAmD,EAAnDH,UAAmD;EACnDI,GAAG,EAAC,cAAc;EAClBF,KAAK,EAAC;;gCAERD,mBAAA,CAA0C;EAApCC,KAAK,EAAC;AAAe,GAAC,SAAO;oBALnCG,UAIE,EACFC,UAA0C,C;;EAEvCJ,KAAK,EAAC;AAAa;mBAX5B;;EAkBaA,KAAK,EAAC;AAAkB;mBAlBrC;;EA0BaA,KAAK,EAAC;AAAa;;uBA1BhCK,mBAAA,CAAAC,SAAA,SACEC,UAA+B,EAC/BR,mBAAA,CA6BM,OA7BNS,UA6BM,GA5BJT,mBAAA,CAOM;IAPDC,KAAK,EAAC,UAAU;IAAES,OAAK,EAAAC,MAAA,QAAAA,MAAA,UAAAC,IAAA,KAAEC,MAAA,CAAAC,MAAA,IAAAD,MAAA,CAAAC,MAAA,IAAAF,IAAA,CAAM;KAHxC,IAAAG,UAAA,IAWIf,mBAAA,CAmBM,OAnBNgB,UAmBM,I,kBAlBJV,mBAAA,CAiBMC,SAAA,QA7BZU,WAAA,CAawBJ,MAAA,CAAAK,MAAM,EAAfC,KAAK;yBADdb,mBAAA,CAiBM;MAfHc,GAAG,EAAED,KAAK,CAACE,EAAE;MACdpB,KAAK,EAAC,YAAY;MACjBS,OAAK,EAAAY,MAAA,IAAET,MAAA,CAAAU,SAAS,CAACJ,KAAK,CAACE,EAAE;QAE1BrB,mBAAA,CAOM,OAPNwB,UAOM,I,kBANJlB,mBAAA,CAKEC,SAAA,QAxBZU,WAAA,CAoBqCE,KAAK,CAACM,MAAM,CAACC,KAAK,QApBvD,CAoBoBC,KAAK,EAAEC,KAAK;2BADtBtB,mBAAA,CAKE;QAHCc,GAAG,EAAEQ,KAAK;QACV1B,GAAG,EAAEyB,KAAK,CAACE,YAAY;QACxB1B,GAAG,EAAC;8BAvBhB2B,UAAA;sCA0BQ9B,mBAAA,CAEM,OAFN+B,WAEM,GADJ/B,mBAAA,CAA0B,YAAAgC,gBAAA,CAAnBb,KAAK,