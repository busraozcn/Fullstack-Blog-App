{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport SidebarComponent from \"./sidebar/SidebarComponent.vue\";\nimport { useRouter } from \"vue-router\";\nimport axios from \"axios\";\nexport default {\n  name: \"AlbumsPage\",\n  components: {\n    SidebarComponent\n  },\n  setup() {\n    const router = useRouter();\n    const goToHome = () => {\n      router.push(\"/\");\n    };\n    const goToAlbum = albumId => {\n      router.push(`/albums/${albumId}`);\n    };\n    return {\n      goToHome,\n      goToAlbum\n    };\n  },\n  created() {\n    const userId = this.$route.query.userId;\n    if (userId) {\n      axios.get(\"https://jsonplaceholder.typicode.com/users/${userId}\").then(response => {\n        this.selectedUser = response.data;\n      }).catch(error => {\n        console.error(\"Error fetching user: \", error);\n      });\n      axios.get(\"https://jsonplaceholder.typicode.com/phos?userId=${userId}\");\n    }\n  }\n};","map":{"version":3,"names":["SidebarComponent","useRouter","axios","name","components","setup","router","goToHome","push","goToAlbum","albumId","created","userId","$route","query","get","then","response","selectedUser","data","catch","error","console"],"sources":["C:\\Users\\Excalibur\\Desktop\\vue-task\\src\\components\\AlbumsPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"albums-page\">\r\n    <SidebarComponent :user=\"selectedUser\" />\r\n    <div class=\"content\">\r\n      <button class=\"geritusu\" @click=\"goToHome\">Geri</button>\r\n      <h1>Albums</h1>\r\n      <div class=\"albums-list\">\r\n        <div\r\n          v-for=\"album in albums\"\r\n          :key=\"album.id\"\r\n          class=\"album-card\"\r\n          @click=\"goToAlbum(album.id)\"\r\n        >\r\n          <img :src=\"album.cover\" alt=\"Album cover\" />\r\n          <h3>{{ album.title }}</h3>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport SidebarComponent from \"./sidebar/SidebarComponent.vue\";\r\nimport { useRouter } from \"vue-router\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"AlbumsPage\",\r\n  components: {\r\n    SidebarComponent,\r\n  },\r\n  setup() {\r\n    const router = useRouter();\r\n\r\n    const goToHome = () => {\r\n      router.push(\"/\");\r\n    };\r\n\r\n    const goToAlbum = (albumId) => {\r\n      router.push(`/albums/${albumId}`);\r\n    };\r\n\r\n    return {\r\n      goToHome,\r\n      goToAlbum,\r\n    };\r\n  },\r\n  created() {\r\n    const userId = this.$route.query.userId;\r\n    if (userId) {\r\n      axios\r\n        .get(\"https://jsonplaceholder.typicode.com/users/${userId}\")\r\n        .then((response) => {\r\n          this.selectedUser = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Error fetching user: \", error);\r\n        });\r\n      axios.get(\"https://jsonplaceholder.typicode.com/phos?userId=${userId}\");\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.albums-page {\r\n  display: flex;\r\n}\r\n\r\n.content {\r\n  margin-left: 260px;\r\n  padding: 20px;\r\n}\r\n\r\n.albums-list {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 20px;\r\n}\r\n\r\n.album-card {\r\n  width: 200px;\r\n  cursor: pointer;\r\n  border: 1px solid #ccc;\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n  text-align: center;\r\n}\r\n\r\n.album-card img {\r\n  width: 100%;\r\n  height: 150px;\r\n  object-fit: cover;\r\n}\r\n\r\n.album-card h3 {\r\n  margin: 10px 0;\r\n  font-size: 18px;\r\n}\r\n</style>\r\n"],"mappings":";AAsBA,OAAOA,gBAAe,MAAO,gCAAgC;AAC7D,SAASC,SAAQ,QAAS,YAAY;AACtC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIL,SAAS,CAAC,CAAC;IAE1B,MAAMM,QAAO,GAAIA,CAAA,KAAM;MACrBD,MAAM,CAACE,IAAI,CAAC,GAAG,CAAC;IAClB,CAAC;IAED,MAAMC,SAAQ,GAAKC,OAAO,IAAK;MAC7BJ,MAAM,CAACE,IAAI,CAAC,WAAWE,OAAO,EAAE,CAAC;IACnC,CAAC;IAED,OAAO;MACLH,QAAQ;MACRE;IACF,CAAC;EACH,CAAC